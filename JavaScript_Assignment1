            console.log(name.charAt(5));
            console.log(name.charCodeAt(5));
            var str1="abcd";
            var str2="efgh";
            var str3="ijkl";
            var res=str1.concat(str2,str3);
            console.log(res);
            var a=String.fromCharCode(65,66,67,68,69);
            console.log(a);
            var text = "Hello world, welcome to the universe.";
            console.log(text.indexOf("e", 5));
            var text = "Hello planet earth, you are a great planet.";
            console.log(text.lastIndexOf("planet", 20));
            
